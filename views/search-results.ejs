<body>
	<!-- Header -->
	<%- include('./partials/header.ejs') %> 
	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Resultados de búsqueda</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<% if (products.length == 0) { %> <h2>No hay resultados para su criterio de búsqueda. </h2> <%} %>


			  <% for(let i=0; i <products.length; i++) { %>
				<% if ( products[i].picture_product) {%>	
				<div class="col-12 col-sm-6 col-lg-3">
						<section class="product-box">
							<h2 class="col-12"> 
								<% if (products[i].picture_product) { %>
								<a href="/products/detail/ <%=products[i].id_product %>">
									<img src="<%= products[i].picture_product %>" width="120" class="product-box_image"/>
								</a>
							  <% } else { %>
								<img  class="product-box_image" src="data:image/png;base64," width="120"/>
							  <% } %>
							  <div class="product-box_data">
								<h2 id="nombre_resultados"><%= products[i].name %></h2>
								<p id="descripcion_resultados">
									<a href="/users/profile/<%= products[i].owner.id_user%>"><%= products[i].owner?.username %></a>
									Description: <%= products[i].description %>
								</p>
								<p><a id="boton_detail" href="/products/detail/ <%=products[i].id_product %>">View details »</a></p>
							  </div>
							</h2>
						</section>
					</div>	
			<% } %> <% }%>
		</div>
	</div>

	<!-- footer -->
	<%- include('./partials/footer.ejs') %> 
	<!-- /footer -->
</body>
</html>